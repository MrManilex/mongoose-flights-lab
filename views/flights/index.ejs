<%- include('../partials/html-head')  %> 
<link rel="stylesheet" href="/stylesheets/flights/index.css">
<%- include('../partials/nav')  %>

<main>
  <h1>All Flights</h1>
  <% flight.forEach(f => { %>
  <ul>
    <li><%= f.airline %> </li>
    <li><%= f.airport %> </li>
    <li><%= f.flightNo %> </li>
    <li><%= f.departs %> </li>
  </ul>
  <a href="/flights/<%= f._id %>">Details</a>
    <% }) %>
</main>

  <%- include('../partials/footer')  %> 
